{# src/AppBundle/Resources/views/Admin/carlogtage.html.twig #} 
{% extends 'admin/init.html.twig' %} 

{% block body %}
	<div id="" class="container">

		<div id="panel" class="panel panel-default">
			<div class="panel-heading">
            		<a class="btn btn-link" href="{{ path('admin_infoholder') }}" style="">Zur√ºck zum Infohoders</a>
            </div>
			<div class="panel-body">
               <form action="{{ path('admin_infoholder_ne',{'eid':infoholder.id}) }}" method="post" id="f-admin-infoholder" data-toggle="validator" role="form">
                	
					 <div class="form-group">
                        <label for="{{ form.quiz.vars.id }}">IH zu Quiz</label>
                        {{ form_widget(form.quiz, { 'attr': { 'class':'select2' } } ) }}
                        <div class="help-block with-errors">
                            {{ form_errors(form.quiz) }}
                        </div>
                    </div>

                	<div class="form-group">
        				<label for="{{ form.tn.vars.id }}">Name</label>
        				{{ form_widget(form.tn) }}
        				<div class="help-block with-errors">{{ form_errors(form.tn) }}</div>
        			</div>
        			
        			<div class="form-group">
        				<label for="{{ form.val.vars.id }}">Wert</label>
						{{ form_widget(form.val, { 'attr': { 'rows':'20' } } ) }}
        				<div class="help-block with-errors">{{ form_errors(form.val) }}</div>
        			</div>
                	
        			<div style="display: none">{{ form_rest(form) }}</div>
            		<div class="form-field">
            			<button class="btn btn-success btn" type="submit">Speichern</button>
            		</div>
                
            		<script type="text/javascript">
            		//
            		$( "#" ).each(function(index) {
            			
            		});
            		</script>
               </form>
    		</div>
		</div>
	</div>
   
{% endblock %} 